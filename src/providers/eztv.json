{
  "name": "EZTV",
  "baseUrl": "https://eztv.io",
  "searchUrl": "/search/{query}",
  "categories": {
    "All": "",
    "TV": ""
  },
  "defaultCategory": "All",
  "resultsPerPageCount": 1000,
  "itemsSelector": "table.forum_header_border tr.forum_header_border",
  "itemSelectors": {
    "title": "a.epinfo@text | trim",
    "time": "td.forum_thread_post:nth-child(5)@text",
    "seeds": "td.forum_thread_post_end@text | int",
    "size": "td.forum_thread_post:nth-child(4)@text",
    "magnet": "a.magnet@href",
    "link": "a.download_1@href",
    "desc": "a.epinfo@href"
  },
  "paginateSelector": "a:contains(Next)",
  "torrentDetailsSelector": "table.forum_header_border_normal:last-of-type"
}
